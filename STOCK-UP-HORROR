maxplayers8.MouseButton1Click:Connect(function()
-- سكربت تحديد لاعبين 8
local ohTable1 = {
	["Map"] = 1,
	["Mode"] = 1,
	["FriendsOnly"] = false,
	["MaxPlayers"] = MaxPlayers
}

game:GetService("ReplicatedStorage").Events.PartyLeader:FireServer(ohTable1)
--
end)

